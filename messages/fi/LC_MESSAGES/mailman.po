msgid "None"
msgstr "Ei mitään"

#: Mailman/Gui/Privacy.py:49 Mailman/Gui/Privacy.py:72
msgid "Confirm"
msgstr "Vahvista"

#: Mailman/Gui/Privacy.py:50 Mailman/Gui/Privacy.py:73
